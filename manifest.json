{
    "name": "Snowflakify",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Turns google into a winter wonderland.",
    "content_scripts": [{
        "matches": ["https://www.google.com/*"],
        "css": ["css/snowflakify.css"],
        "js": ["./dist/index.js"]
    }],
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_icon": "images/snowflake.png",
        "default_title": "Snowflakify"
    },
    "web_accessible_resources": [
        "images/snowflake.png"
    ],
    "background": {
        "page": "./background.html",
        "persistent": true
    },
    "icons": {
        "128": "images/snowflake.png"
    },
    "options_page": "./options.html"
}
